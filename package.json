{
  "name": "blog",
  "version": "1.0.0",
  "scripts": {
    "test": "tsc",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "push": "git add -A && git commit -m update && git push",
    "start": "next start --port 3100",
    "export": "npm run build && next export",
    "deploy": "rm -rf node_modules/.cache && npm run export && touch out/.nojekyll && echo 17forever.me > out/CNAME && git add out/ -f && git commit -m \"Deploy out files to gh-pages\" && git subtree split --prefix out -b gh-pages_local && git push -f origin gh-pages_local:gh-pages && git branch -D gh-pages_local",
    "type-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/17forever/blog.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/17forever/blog/issues"
  },
  "homepage": "https://github.com/17forever/blog#readme",
  "dependencies": {
    "@fluentui/react": "^7.133.0",
    "@node-rs/jieba": "^0.6.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.1.2",
    "immutable": "^4.0.0-rc.12",
    "markdown-it": "^11.0.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-link-attributes": "^3.0.0",
    "next": "^9.5.2",
    "normalize.css": "^8.0.1",
    "pinyin": "^2.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-responsive": "^8.1.0",
    "react-tag-cloud": "^1.3.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-alloy": "^3.8.0",
    "eslint-plugin-react": "^7.20.6",
    "typescript": "^4.0.2"
  }
